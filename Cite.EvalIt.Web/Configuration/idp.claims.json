{
	"IdpClient": {
		"Claims": {
			"Mapping": {
				"JWTID": [ { "Type": "jti" } ],
				"ExpiresAt": [ { "Type": "exp" } ],
				"NotBefore": [ { "Type": "nbf" } ],
				"IssuedAt": [ { "Type": "iat" } ],
				"Issuer": [ { "Type": "iss" } ],
				"Audience": [ { "Type": "aud" } ],
				"Subject": [ { "Type": "sub" } ],
				"TokenType": [ { "Type": "typ" } ],
				"AuthorizedParty": [ { "Type": "azp" } ],
				"AuthenticatedAt": [ { "Type": "auth_time" } ],
				"Roles": [
					{ "Type": "role" }
					//,{
					//	"Type": "realm_access",
					//	"Path": "roles",
					//	"Prefix": "realm_"
					//},
					//{
					//	"Type": "resource_access",
					//	"Path": "data-validation.roles",
					//	"Prefix": "evalit_"
					//}
				],
				"Scope": [ { "Type": "scope" } ],
				"Client": [ { "Type": "clientId" } ],
				"ClientHost": [ { "Type": "clientHost" } ],
				"ClientAddress": [ { "Type": "clientAddress" } ],
				"PreferredUsername": [ { "Type": "preferred_username" } ],
				"EmailVerified": [ { "Type": "email_verified" } ],
				"Email": [ { "Type": "email" } ],
				"Name": [ { "Type": "name" } ],
				"GivenName": [ { "Type": "given_name" } ],
				"FamilyName": [ { "Type": "family_name" } ],
				"AuthenticationMethod": [ { "Type": "amr" } ],
				"AccessToken": [
					{
						"Type": "x-access-token",
						"Visibility": "Sensitive"
					}
				],
				"Tenant": [ { "Type": "x-tenant" } ]
			}
		}
	}
}